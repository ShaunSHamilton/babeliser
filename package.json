{
  "author": "Shaun Hamilton",
  "bugs": {
    "url": "https://github.com/ShaunSHamilton/babeliser/issues"
  },
  "description": "A blazingly brilliant code parser for testing JavaScript code",
  "devDependencies": {
    "@babel/types": "7.19.4",
    "@types/chai": "4.3.3",
    "@types/mocha": "10.0.0",
    "chai": "4.3.6",
    "esbuild": "0.15.11",
    "logover": "2.0.0",
    "mocha": "10.1.0",
    "typescript": "4.8.4"
  },
  "homepage": "https://github.com/ShaunSHamilton/babeliser#readme",
  "keywords": [
    "babel",
    "js",
    "node",
    "nodejs"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "name": "babeliser",
  "peerDependencies": {
    "@babel/parser": "7.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShaunSHamilton/babeliser.git"
  },
  "scripts": {
    "build": "esbuild --tsconfig=tsconfig.json --platform=neutral --main-fields=main --bundle --minify --outdir=lib src/index.ts && npx tsc",
    "prepub": "npm run build",
    "pub": "npm publish",
    "test": "esbuild --tsconfig=tsconfig.json --platform=neutral --external:@babel/parser --external:@babel/types --external:chai --bundle --external:logover --outdir=temp-test test/index.ts && mocha temp-test/index.js"
  },
  "type": "module",
  "types": "lib/index.d.ts",
  "version": "0.4.0"
}
